    </test>
    <test name="Multiplication Calculator Test">
        <groups>
            <run>
                <exclude name="first"/>
            </run>
        </groups>
        <classes>
            <class name="testng.MultiplicationCalculatorTest"/>
        </classes>
    </test>
</suite>